#include <iostream>
using namespace std;

int main()
{
    int numbers[4];
    
    for (int i = 0; i < 4; i++) {
        cin >> numbers[i];
    }

    int max = numbers[0];
    int min = numbers[0];
    
    for (int i = 0; i < 4; i++)
    {
        if (numbers[i] < min)
        {
            min = numbers[i];
        }
        if (numbers[i] > max)
        {
            max = numbers[i];
        }
    }
    cout << "The smallest number is :  " << min << "\n";
    cout << "The greater number is :  " << max;
    return 0;
}
